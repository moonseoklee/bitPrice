{
  "manifest_version": 2,
  "name": "Bitprice",
  "version": "1.5.0",
  "description": "show bitprice simply",
  "background":{
    "scripts":["/src/constants/background.js"]
  },
  "permissions":
    ["https://api.bitfinex.com/"]
  ,
  "icons": {
    "32": "src/imgs/sBitcoin.png",
    "128": "src/imgs/bBitcoin.png"
  },
  "sidebar_action": {
    "default_page": "index.html",
    "default_icon": {
      "32": "src/imgs/sBitcoin.png"
    },
    "default_title": "bitprice",
    "use_navigation_bar": false
  }
}
